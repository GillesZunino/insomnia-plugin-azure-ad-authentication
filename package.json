    "clean-webpack-plugin": "4.0.0",
    "terser-webpack-plugin": "5.3.10",
    "ts-loader": "9.5.1",
    "tsdx": "0.14.1",
    "typescript": "5.4.4",
    "webpack": "5.91.0",
    "webpack-cli": "5.1.4",
    "webpack-merge": "5.10.0",
    "webpack-node-externals": "3.0.0"
  },
  "peerDependencies": {}
